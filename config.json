{
  "database": {
    "createDatabase": true,
    "enableLogging": true,
    "storagePath": "./data/db.sqlite3"
  },

  "hostManager": {
    "cleanOnStart": false,
    "faviconPath": "./dist/favicons/"
  },

  "httpServer": {
    "port": 8080
  },

  "importer": {
    "filePath": "./data/examples.txt"
  },

  "processor": {
    "contentPath": "./data/content/",
    "textContentPath": "./data/textcontent/",
    "cleanOnStart": false
  },

  "requestManager": {
    "requestTimeout": 5000
  }
}
